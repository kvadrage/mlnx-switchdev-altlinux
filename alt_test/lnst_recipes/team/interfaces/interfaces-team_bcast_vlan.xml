<interfaces>
    <eth id="1" label="net1"/>
    <eth id="2" label="net1"/>
    <eth id="3" label="net1"/>
    <team id="team">
        <options>
            <!--"hwaddr": "00:11:22:33:44:55",-->
            <option name="teamd_config">
                {
                    "runner": {"name": "broadcast"}
                }
            </option>
        </options>
        <slaves>
            <slave id="1"/>
            <slave id="2"/>
            <slave id="3"/>
        </slaves>
    </team>
    <vlan id="testiface">
        <options>
            <option name="vlan_tci" value="100"/>
        </options>
        <slaves>
            <slave id="team"/>
        </slaves>
        <addresses>
             <address value="{$testip}"/>
             <address value="{$testip6}"/>
        </addresses>
    </vlan>
</interfaces>
